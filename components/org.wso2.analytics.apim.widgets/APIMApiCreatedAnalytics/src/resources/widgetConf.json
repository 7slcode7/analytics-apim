{
  "name": "APIM Api Created Analytics",
  "id": "APIMApiCreatedAnalytics",
  "thumbnailURL": "",
  "configs": {
    "pubsub": {
      "types": ["subscriber"]
    },
    "providerConfig" : {
      "configs": {
        "type": "RDBMSStreamingDataProvider",
        "config": {
          "datasourceName": "AM_DB",
          "queryData": {
            "query" : "select API_ID, API_NAME, API_VERSION, CREATED_TIME, COUNT(API_ID), CREATED_BY as count from AM_API where CONTEXT {{context_condition}} {{created_by}} AND CREATED_TIME between '{{timeFrom}}' AND '{{timeTo}}' group by CREATED_TIME"
          },
          "tableName": "AM_API",
          "incrementalColumn": "CREATED_TIME",
          "publishingLimit": 2147483647,
          "isPurgingEnable": false
        }
      }
    },
    "options": [
      {
        "id": "header",
        "title": "Header",
        "type": {
          "name": "BOOLEAN",
          "possibleValues": [
            true,
            false
          ]
        },
        "defaultValue": true
      }
    ]
  }
}
